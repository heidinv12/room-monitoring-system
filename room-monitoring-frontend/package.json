{
  "name": "es6-starter",
  "version": "0.1.0",
  "description": "Project starter for small frontend projects: webpack, babel, eslint, jest, and more",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --inline --port 9000",
    "build": "webpack --config webpack.prod.js",
    "test": "jest -w 1",
    "lint": "eslint src tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MattBroach/es6-starter.git"
  },
  "keywords": [
    "webpack",
    "babel",
    "eslint",
    "jest",
    "sass",
    "es6",
    "starter"
  ],
  "author": "Matt Nishi-Broach",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MattBroach/es6-starter/issues"
  },
  "homepage": "https://github.com/MattBroach/es6-starter#readme",
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-jest": "^2.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
